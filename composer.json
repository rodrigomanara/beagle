{
    "name": "youcanadin/beagle",
    "authors": [
        {
            "name": "Rodrigo Manara",
            "email": "me@rodrigomanara.co.uk"
        }
    ],
    "keywords": ["beagle", "youcanadmin", "rmanara", "status", "page"],
    "type": "project",
    "require": {
        "symfony/http-foundation": "3.1.*",
        "symfony/var-dumper": "3.1.*",
        "symfony/routing": "3.1.*",
        "twig/twig": "^1.28", 
        "symfony/finder": "3.1.*",
        "symfony/console": "3.1.*",
        "phpmailer/phpmailer": "~5.2",
        "mustangostang/spyc": "^0.6.1",
        "monolog/monolog": "^1.22",
        "phpunit/phpunit": "5.7.0" 
    },
    "autoload": {
        "psr-4": {
            "Beagle\\": "bin/",
            "Block\\": "src/Block" 
        }
    }, 
    "autoload-dev": {
        "psr-4": {
            "Beagle\\Test\\": "test/"
        }
    },
    "archive": {
        "exclude": ["vendor", "test"] 
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

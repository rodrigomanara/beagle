{# empty Twig template #}
<html>

    <head>
        <base href="{{ base_url }}"/>
        <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link type="text/css" rel="stylesheet" href="bin/CMS/View/public/css/style.css"  media="screen,projection"/>
        <link type="text/css" rel="stylesheet" href="bin/CMS/View/public/materialize/css/materialize.min.css"  media="screen,projection"/>
        <!--Let browser know website is optimized for mobile-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        {% block header %}
        {% endblock header %}
    </head>
    <body>
        {% block container %}
            {% block nav %} 
            {% endblock nav %}

            <div class="row remove-row-margin">

                <div class="col s3">
                </div>
                <div class="col s9">
                    <div class="height-100">

                        {% block body %}

                        {% endblock body %}
                    </div>
                </div>

            </div>
        {% endblock container %}  
    </body>
    <footer> 
        <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
        <script src="bin/CMS/View/public/materialize/js/materialize.min.js"></script>

        {% block footer %}

        {% endblock footer %}  

    </footer>
</html>